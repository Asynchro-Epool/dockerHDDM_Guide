T <- c(1:2000)
w <- data.frame(Time = t, w = NA)
t0 = 0
w0 = 0
for (i in 1:2000){
t <- t0 + T[i]
w_delta <- rnorm(1, 0, 1)
if (i <= 1){
w$w[i] <- w0 + w_delta
} else {
w$w[i] <- w$w[i-1] +  w_delta
}
}
plot(w$Time, w$w)
# Time
T <- c(1:2000)
w <- data.frame(Time = t, w = NA)
t0 = 0
w0 = 0
for (i in 1:2000){
t <- t0 + T[i]
w_delta <- rnorm(1, 0, 1)
w_delta <- w_delta*sqrt(i)
if (i <= 1){
w$w[i] <- w0 + w_delta
} else {
w$w[i] <- w$w[i-1] +  w_delta
}
}
rm(w,i,t,T,t0,w_delta,w0)
